#:kivy 1.8.0
<weatherForcast_Detect>:
	BoxLayout:
		orientation: 'vertical'
		Button:
			text: 'Weather Forcast')

<fishingLocations>:
	BoxLayout:
		orientation: 'vertical'
		Button:
			text: 'Fishing locations')

<catchRecord>:
    txt_inpt: txt_inpt
    Button:
        id: f_but
    TextInput:
        id: txt_inpt
        text: f_but.state
on_text: root.check_status(f_but)
